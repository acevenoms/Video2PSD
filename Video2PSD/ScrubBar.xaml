<UserControl x:Class="Video2PSD.ScrubBar"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="30" d:DesignWidth="301">
    <Grid>
        <Rectangle Width="Auto" Fill="Aqua" Height="3" Stretch="UniformToFill"/>
        <Rectangle Width="11" Height="11" Fill="Coral"  />
        <Line X1="151" Y1="0" X2="151" Y2="30" Stroke="AntiqueWhite" StrokeThickness="1"/>
    </Grid>
</UserControl>
